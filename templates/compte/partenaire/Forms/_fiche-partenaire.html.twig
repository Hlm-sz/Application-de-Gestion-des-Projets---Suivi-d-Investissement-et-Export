					</div>
				</div>
			</div>
			<div class="row align-items-end mb-2">
				<div class="col-lg-4">
						<div class="form-group mb-0">
						<label class="cs-label">Logo</label>
						{{ form_widget(form.logo) }}
					</div>
					<div class="form-group mb-0">
						<label class="cs-label">Pays d'origine</label>
						{{ form_widget(form.paysSiege ,{'attr': {'class': 'form-group siege'}}) }}

					</div>
					<div class="form-group mb-0">
						<label class="cs-label">Pays cible</label>
						{{ form_widget(form.paysImplantationSuccursales ,{'attr': {'class': 'form-group sales'}}) }}

					</div>
				</div>

				<div class="col-lg-8">
					<div class="form-group mb-2">
						<label class="cs-label mb-0">Description de l’activité</label>
						{{ form_widget(form.description_partenaire) }}
					</div>
				</div>
			</div>
			<div class="row align-items-end mb-2">
				<div class="col-lg-4">
					<div class="form-group mb-0">
						<label class="cs-label">Secteur</label>
 						{{ form_widget(form.secteurs ,{'attr': {'class': 'form-group Secteur'}}) }}

					</div>
					<div class="form-group mb-0">
						<label class="cs-label">Présentation</label>
						{{ form_widget(form.presentation) }}
					</div>
				</div>
				<div class="col-lg-8">
					<div class="form-group mb-2">
						<label class="cs-label mb-0">Détails libres</label>
						{{ form_widget(form.detail_libre) }}
					</div>
				</div>
			</div>
