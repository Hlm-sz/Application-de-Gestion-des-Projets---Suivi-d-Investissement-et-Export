<div class="tab-content cs-tab-content no-left-padding no-right-padding info-displayed" id="info-parte">
	<div class="tab-pane fade show active" id="item-details" role="tabpanel">
		<div class="container-fluid">
			<div class="row align-items-end mb-2">
				<div class="col-lg-3">
					<div class="form-group mb-0">
						<label class="cs-label">Nom du Partenaire</label>
						{{ form_widget(form.nomCompte) }} 
						{{ form_errors(form.nomCompte) }}
					</div>
				</div>
				<div class="col-lg-3">
					<div class="form-group mb-0">
						<label class="cs-label">Type</label>
						{{ form_widget(form.TypePartenaire) }}
					</div>
				</div>
				<div class="col-lg-3">
					<div class="form-group mb-2 d-flex justify-content-between align-items-center">
						<label class="cs-label">Engagement</label>
						{{ form_widget(form.EngagementPart, {label:false,label_attr: {class: 'switch-custom'} }) }}
					</div>
				</div>
				<div class="col-lg-3">
					<div class="form-group mb-0" id="engage">
						<label class="cs-label">Type d’Engagement</label>
