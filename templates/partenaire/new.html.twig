{% extends 'base.html.twig' %}

{% block title %}
	{{ 'partenaire.New_partenaire'| trans  }}
{% endblock %}
{% block sous_title %}
<a class="navbar-brand" href="{{ path('partenaire_index') }}">
	<img src="{{ asset('images/amdie.png') }}" width="30" height="30" class="d-inline-block align-top"
		alt="logo">
		<div class="SousTitre">
		<strong>AMDIE</strong>
	|
	{{ 'partenaire.Partenaires'| trans | upper }}
	</div>
</a>
{% endblock %}
{% block navbar_right %}

	<a class="btn btn-blue" href="#" id="save_partenaire">
		<i class="fa fa-save"></i>
	</a>
{% endblock %}

{% block navbar_left %}
	<a class="btn btn-blue" href="{{ app.request.headers.get('referer') }}">
		<i class="fa fa-arrow-left"></i>
	</a>
