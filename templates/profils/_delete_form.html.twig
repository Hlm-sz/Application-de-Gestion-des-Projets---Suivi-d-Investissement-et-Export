<form method="post" id="delete-form" action="{{ path('profils_delete', {'id': profil.id}) }}">
	<input type="hidden" name="_method" value="DELETE">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ profil.id) }}">
	<button class="btn btn-blue" id="delete-item">{{ 'profil.Delete'| trans }}</button>
</form>
