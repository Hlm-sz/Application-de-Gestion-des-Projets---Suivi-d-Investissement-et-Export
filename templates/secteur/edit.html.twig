{% extends 'base.html.twig' %}

{% block title %}
	{{ 'secteur.Edit_secteur'| trans }}
{% endblock %}
{% block sous_title %}
<a class="navbar-brand" href="{{ path('secteur_index') }}">
	<img src="{{ asset('images/amdie.png') }}" width="30" height="30" class="d-inline-block align-top"
		alt="logo">
		<div class="SousTitre">
		<strong>AMDIE</strong>
	|
	{{ 'secteur.Secteur'| trans | upper }}
	</div>
</a>
{% endblock %}

{% block navbar_right %}{% endblock %}
{% block navbar_left %}
	<a class="btn btn-blue" href="{{ path('secteur_index') }}">{{'secteur.Back_to_list'| trans}}</a>
	<h4 class="navbar-text-color">{{'secteur.Edit_secteur'| trans}}</h4>
{% endblock %}

{% block body %}

	<div class="card">
		<div class="card-body">
			{{ include('secteur/_form.html.twig', {'button_label': 'secteur.Update'| trans }) }}
		</div>
	</div>

{% endblock %}
